{
  "name": "mock_server",
  "version": "1.0.0",
  "description": "Simple mock service",
  "author": "Juan Pinheiro",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/juanpinheiro/mock-server.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "mock"
    ]
  },
  "scripts": {
    "test": "mocha -R spec app/**/*.spec.js",
    "start": "./node_modules/.bin/forever app/index.js",
    "postinstall": "NODE_PATH=web/src/ react-scripts build && cp -r web/src/build/. public_html && rm -R web/src/build && echo React build completed",
    "lint": "standard --verbose | snazzy",
    "node-debug": "DEBUG=express* node app/index.js",
    "chrome-debug": "node --debug-brk --inspect app/index.js",
    "coverage": "istanbul cover _mocha $(find ./app -name \"*.spec.js\" -not -path \"./node_modules/*\") ",
    "metric": "nodejs-dashboard -- node -r nodejs-dashboard app/index.js"
  },
  "devDependencies": {
    "eslint": "^3.13.1",
    "mocha": "^3.2.0",
    "nodejs-dashboard": "^0.3.0",
    "should": "^11.1.2",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "supertest": "^2.0.1"
  },
  "dependencies": {
    "awilix": "~2.12.0",
    "cors": "^2.8.4",
    "express": "^4.14.0",
    "forever": "^0.15.3",
    "helmet": "^3.4.0",
    "http-status": "^0.2.5",
    "joi": "^13.0.2",
    "lodash": "^4.17.4",
    "mongodb": "^2.1.18",
    "morgan": "^1.7.0",
    "q": "^1.5.1",
    "axios": "^0.17.1",
    "material-ui": "^0.19.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "spdy": "https://registry.npmjs.org/spdy/-/spdy-3.4.4.tgz"
  },
  "license": "ISC",
  "homepage": "https://github.com/juanpinheiro/mock-server.git"
}
